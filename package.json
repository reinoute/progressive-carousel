{
  "name": "progressive-carousel",
  "version": "1.1.0",
  "description": "A mobile friendly and progressive enhanced image carousel",
  "moduleName": "Carousel",
  "main": "dist/progressive-carousel.js",
  "module": "dist/progressive-carousel.module.js",
  "jsnext:main": "dist/progressive-carousel.es.js",
  "umd:main": "dist/progressive-carousel.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reinoute/progressive-carousel.git"
  },
  "keywords": [
    "carousel",
    "progressive",
    "enhancement"
  ],
  "author": "Reinout Eppinga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reinoute/progressive-carousel/issues"
  },
  "homepage": "http://www.reinout.com/progressive-carousel/",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "rollup --config rollup.config.js --environment NODE_ENV:production",
    "watch": "npm run watch:js",
    "watch:js": "rollup --config rollup.config.js --watch",
    "server": "python -m SimpleHTTPServer 3000"
  },
  "devDependencies": {
    "rollup": "^0.45.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {
    "fscreen": "^1.0.2"
  }
}
