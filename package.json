{
  "name": "progressive-carousel-demo",
  "version": "1.0.0",
  "description": "A mobile friendly and progressive enhanced image carousel",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reinoute/progressive-carousel.git"
  },
  "keywords": [
    "carousel",
    "progressive",
    "enhancement"
  ],
  "author": "Reinout Eppinga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reinoute/progressive-carousel/issues"
  },
  "homepage": "https://github.com/reinoute/progressive-carousel#readme",
  "scripts": {
    "build:js": "browserify ./src/index.js -o ./index.js -p [minifyify --no-map] -t [ babelify --presets [es2015] --plugins transform-class-properties]",
    "watch:js": "watchify ./src/index.js -o ./index.js -t [ babelify --presets [es2015] --plugins transform-class-properties] -v",
    "server": "python -m SimpleHTTPServer 3000"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "minifyify": "^7.3.4",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "progressive-carousel": "^1.5.1"
  }
}
